/*					main toolbar					*/
QMainWindow QToolBar#toolbar
{
border:none;
height:100%;
padding-top:20px; 
background: transparent; 
text-align: left; 
/*color: white;*/
min-width:180px;
max-width:180px;
}

QMainWindow QToolBar#toolbar QToolButton:hover 
{
border: 1px solid #088af8;
} 

QMainWindow QToolBar#toolbar QToolButton:checked 
{
background: #b1b1b1;
border: 2px solid #088af8;
}  

QMainWindow QToolBar#toolbar QToolButton 
{
font: bold 12px;
font-family:'Sans';
padding-right: 65px;
padding-top:4px;
padding-bottom:4px;
margin-bottom: 4px; 
width:200px; 
/*color: white; */
text-align: right; 
background:transparent;
text-transform:uppercase; 
/*border-radius: 5px;*/
border: 1px solid #b1b1b1
}

QMainWindow QToolBar#toolbar
{
width: 220px;
}

/*				toolbar 2 - bottom			*/
QMainWindow QToolBar#toolbar2
{
background: #b1b1b1;
/*border: 1px solid #088af8;*/
padding-left:20px; 
width:950px;
margin:50px;
height:30px;
}

QMainWindow QToolBar#toolbar2 QToolButton 
{
background: #b1b1b1;
border: none;
padding-left:8px;
padding-right:8px;
height:30px;
width:30px;
}

QMainWindow QToolBar#toolbar2 QToolButton:hover 
{
background: #323232;
}

QMainWindow QToolBar#toolbar2:item
{
    background: transparent;
}


